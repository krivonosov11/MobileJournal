<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bogda.mobilejournal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".User_Activity"
            android:label="Учень" />
        <activity
            android:name=".SettingActivity"
            android:label="Налаштування" />
        <activity
            android:name=".days.MondayActivity"
            android:label="@string/monady" />
        <activity
            android:name=".days.TuesdayActivity"
            android:label="@string/tuesday" />
        <activity
            android:name=".days.WednesdayActivity"
            android:label="@string/wednesday" />
        <activity
            android:name=".days.ThursdayActivity"
            android:label="@string/thursday" />
        <activity
            android:name=".days.FridayActivity"
            android:label="@string/friday" />
        <activity android:name=".Add_Subject" />
        <activity android:name=".Add_NewTable"></activity>
    </application>

</manifest>